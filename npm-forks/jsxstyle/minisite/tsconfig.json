{
  "extends": "../config/tsconfig.base.json",
  "include": [
    "src"
  ],
  "compilerOptions": {
    "rootDir": ".",
    "tsBuildInfoFile": "dist/cache.tsbuildinfo",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "strict": true,
    "moduleResolution": "bundler",
    "jsx": "react-jsx",
    "target": "esnext",
    "module": "esnext",
    "paths": {
      "jsxstyle": [
        "../packages/jsxstyle"
      ]
    }
  },
  "references": [
    {
      "path": "../packages/jsxstyle-bundler-utils"
    },
    {
      "path": "../packages/jsxstyle-react"
    },
  ]
}
