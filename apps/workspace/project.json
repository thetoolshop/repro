{
  "name": "@repro/workspace",
  "targets": {
    "build": {
      "executor": "nx:run-script",
      "options": {
        "script": "build"
      },
      "outputs": ["{projectRoot}/dist"],
      "dependsOn": ["^build"],
      "cache": true
    },
    "serve": {
      "executor": "nx:run-script",
      "options": {
        "script": "serve"
      },
      "dependsOn": ["^build"]
    }
  },
  "tags": ["type:app", "build:docker"],
  "services": [
    "workspace"
  ]
}
